install.packages("read.transactions")
install.packages("arules")
library("arules")
koszyk = read.csv('koszyk.csv')
koszyk[,4] <- as.factor(koszyk[,4])
trans <- as(koszyk[,-1],"transactions")
rules <- apriori(trans, parameter = list(supp = 0.0001, conf = 0.8))
wynik <- as(rules,"data.frame")
write.csv(wynik,"wynik.csv")
